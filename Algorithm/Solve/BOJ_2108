from collections import Counter
import sys
input = sys.stdin.readline

n = int(input())
nums = sorted([int(input()) for _ in range(n)])

avg = round(sum(nums) / len(nums))
median = nums[len(nums)//2]

cnts = Counter(nums).most_common()
if len(cnts) > 1:
    if cnts[0][1] == cnts[1][1]:
        mode = cnts[1][0]
    else:
        mode = cnts[0][0]
else:
    mode = cnts[0][0]

range = nums[-1] - nums[0]

print(avg)
print(median)
print(mode)
print(range)